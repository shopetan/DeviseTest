production:
  adapter: mysql2
  database: devisetest_pro
  host: <%= ENV["RAILS_DATABASE_HOST"] %>
  username: <%= ENV["RAILS_DATABASE_USERNAME"] %>
  password: <%= ENV["RAILS_DATABASE_PASSWORD"] %>
  encoding: utf8

development:
  adapter: mysql2
  database: devisetest_pro
  host: localhost
  username: root
  password: ""
  encoding: utf8

